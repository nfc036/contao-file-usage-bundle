{
    "name": "nfc036/contao-file-usage-bundle",
    "type": "contao-bundle",
    "description": "File Usage extension for Contao Open Source CMS",
    "homepage": "https://contao.org",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Steffen Bock",
            "homepage": "https://github.com/nfc036"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "~4.9"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.12",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Nfc036\\ContaoFileUsageBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Nfc036\\ContaoFileUsageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nfc036\\ContaoFileUsageBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/nfc036/contao-file-usage-bundle/issues",
        "source": "https://github.com/nfc036/contao-file-usage-bundle"
    }
}
